<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Methods for checking positive or negative number" parallel="tests" thread-count="10">
    <test name="Test classes for other methods">
        <groups>
            <run>
                <include name="other"/>
            </run>
        </groups>
        <classes>
            <class name="testng.tests.IsNegativeTest"/>
            <class name="testng.tests.IsPositiveTest"/>
        </classes>
    </test>
</suite>