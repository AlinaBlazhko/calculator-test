<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite">
    <listeners>
        <listener class-name="listener.TestListeners"/>
    </listeners>
    <test name="regression">
        <groups>
            <run>
                <include name="trigonometric"/>
            </run>
        </groups>
        <classes>
            <class name="tests.CosTest"/>
            <class name="tests.SinTest"/>
        </classes>
    </test>
</suite>