<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Trigonometric methods" parallel="tests" thread-count="5">
    <test name="Test classes for trigonometric methods">
        <groups>
            <run>
                <include name="trigonometric"/>
            </run>
        </groups>
        <classes>
            <class name="testng.tests.CosTest"/>
            <class name="testng.tests.CtgTest"/>
            <class name="testng.tests.SinTest"/>
            <class name="testng.tests.TgTest"/>
        </classes>
    </test>
</suite>